<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="org.tensorflow.lite.examples.CurrencyDectector">

    <uses-sdk />

    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />


    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_currency"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_currency"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="org.tensorflow.lite.examples.CurrencyDectector.ClassifierActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            tools:ignore="Instantiatable">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
</manifest>
